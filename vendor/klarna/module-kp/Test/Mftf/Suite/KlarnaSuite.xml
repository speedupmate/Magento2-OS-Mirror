<suites xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Suite/etc/suiteSchema.xsd">
    <suite name="KlarnaPaymentsAT">
        <before>
            <actionGroup ref="KpLoginAsAdmin" stepKey="kpLoginAsAdmin"/>
            <actionGroup ref="ConfigureKlarnaEU" stepKey="kpConfigureCredentials"/>
            <actionGroup ref="KpSignOut" stepKey="kpSignOutFromAdmin"/>
        </before>
        <after>
            <actionGroup ref="KpLoginAsAdmin" stepKey="kpLoginAsAdmin"/>
            <actionGroup ref="DisableKlarnaPayment" stepKey="disableKlarnaPayment"/>
            <actionGroup ref="KpSignOut" stepKey="kpSignOutFromAdmin"/>
        </after>
        <include>
            <module name="Klarna_Kp" />
        </include>
        <exclude>
            <group name="skip"/>
        </exclude>
    </suite>
