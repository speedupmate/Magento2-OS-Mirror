{
    "name": "vertexinc/sdk",
    "description": "Tools for communicating with Vertex Cloud and Vertex O-Series",
    "type": "library",
    "license": "proprietary",
    "version": "1.2.8",
    "require": {
        "php": "^5.4|^7",
        "ext-soap": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*"
    },
    "archive": {
        "exclude": [
            ".github",
            ".git",
            ".gitattributes",
            ".gitignore",
            ".travis.yml",
            "phpcs.xml",
            "phpcs-old.xml",
            "phpcs-new.xml"
        ]
    },
    "autoload": {
        "psr-4": {
            "Vertex\\": "src/"
        }
    },
    "require-dev": {
        "php": "^7.3",
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^9",
        "phpmd/phpmd": "^2.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "mediotype/code-standard": "^1.0"
    },
    "replace": {
        "vertex/sdk": "self.version"
    },
    "autoload-dev": {
        "psr-4": {
            "Vertex\\Test\\": "tests/"
        }
    }
}

