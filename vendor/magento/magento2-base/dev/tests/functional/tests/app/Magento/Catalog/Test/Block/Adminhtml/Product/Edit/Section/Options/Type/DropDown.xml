<mapping strict="0">
    <fields>
        <title>
            <selector>[name$="[title]"]</selector>
        </title>
        <use_default_title>
            <selector>[name^="options_use_default"]</selector>
            <strategy>css selector</strategy>
            <input>checkbox</input>
        </use_default_title>
        <price>
            <selector>[name$="[price]"]</selector>
        </price>
        <price_type>
            <selector>[name$="[price_type]"]</selector>
            <input>select</input>
        </price_type>
        <sku>
            <selector>[name$='[sku]']</selector>
        </sku>
    </fields>
</mapping>
