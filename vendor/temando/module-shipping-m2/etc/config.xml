<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <temando>
                <active>0</active>
                <title>Magento Shipping</title>
                <sallowspecific>0</sallowspecific>
                <active_rma>0</active_rma>
                <collectionpoints_enabled>0</collectionpoints_enabled>
                <collectionpoints_countries>US</collectionpoints_countries>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
                <model>Temando\Shipping\Model\Shipping\Carrier</model>
                <register_account_url>https://account.magento.com/shipping/onboarding/start</register_account_url>
                <shipping_portal_url>https://shipping.magento.com/</shipping_portal_url>
                <sync_enabled>0</sync_enabled>
                <sync_stream_id>magento2</sync_stream_id>
                <api_version>1.0</api_version>
                <session_endpoint></session_endpoint>
                <sovereign_endpoint></sovereign_endpoint>
            </temando>
        </carriers>
        <sales_email>
            <shipment>
                <cancel_template>sales_email_shipment_cancel_template</cancel_template>
                <cancel_guest_template>sales_email_shipment_cancel_guest_template</cancel_guest_template>
            </shipment>
            <temando_pickup>
                <enabled>1</enabled>
                <ready_template>sales_email_temando_pickup_ready_template</ready_template>
                <collected_template>sales_email_temando_pickup_collected_template</collected_template>
                <canceled_template>sales_email_temando_pickup_canceled_template</canceled_template>
                <identity>sales</identity>
                <copy_method>bcc</copy_method>
            </temando_pickup>
        </sales_email>
    </default>
</config>
