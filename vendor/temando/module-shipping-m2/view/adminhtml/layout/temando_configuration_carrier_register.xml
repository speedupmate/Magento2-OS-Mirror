<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Temando_Shipping::static/css/main.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Temando\Shipping\Block\Adminhtml\ComponentContainer" name="admin.temando.carrier.init">
                <arguments>
                    <argument name="template" xsi:type="string">Temando_Shipping::components/configuration/carrier/register.phtml</argument>
                    <argument name="viewModel" xsi:type="object">Temando\Shipping\ViewModel\Carrier\CarrierRegistration</argument>
                    <argument name="jsLayout" xsi:type="array">
                        <item name="components" xsi:type="array">
                            <item name="temando-componentry" xsi:type="array">
                                <item name="component" xsi:type="string">uiComponent</item>
                                <item name="config" xsi:type="array">
                                    <item name="template" xsi:type="string">Temando_Shipping/components/init</item>
                                </item>
                                <item name="children" xsi:type="array">
                                    <item name="init" xsi:type="array">
                                        <item name="sortOrder" xsi:type="string">1</item>
                                        <item name="component" xsi:type="string">Temando_Shipping/js/view/components/init</item>
                                    </item>
                                </item>
                            </item>
                        </item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
